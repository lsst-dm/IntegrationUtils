<file>
  <raw>
      filename = /blue-ccd-data/mengel/work/crosstalk_wrap/run/decam-sispi-00022719.fits.fz
  </raw>
  <split_ccd>
      filename = ${file.raw.filename:trim}_${ccd}.fits
      ccd = (1,3,5,7-30,33-40):2
  </split_ccd>
</file>
<exec_1>
  parents= file.raw
  children = file.split_ccd*
  command = DECam_crosstalk 
  cmdargs = ${file.raw.filename},${file.raw.filename:trim}
  <cmdopts>
    maxhdunum = 63 
    verbose = 3 
    crosstalk = /blue-ccd-data/mengel_work/cal/DECam.xtalk 
    photflag = 1
  </cmdopts>
</exec_1>
