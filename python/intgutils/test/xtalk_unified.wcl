      filename = /blue-ccd-data/mengel/work/crosstalk_wrap/run/decam-sispi-00022719.fits.fz
  </raw>
  <split_ccd>
      filename = ${files.raw.filename:trim}_${ccd}.fits
      ccd = (1-62):2
  </split_ccd>
</files>
<exec_1>
  parents= files.raw
  children = files.split_ccd*
  command = DECam_crosstalk 
  cmdargs = ${files.raw.filename},${files.raw.filename:trim}
  <cmdopts>
    maxhdunum = 63 
    verbose = 3 
    crosstalk = /blue-ccd-data/mengel_work/cal/DECam.xtalk 
    photflag = 1
  </cmdopts>
</exec_1>
